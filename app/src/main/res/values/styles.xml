<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="GetMeThemeQRecorder" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="color_background_default_get_me">@android:color/transparent</item>
        <item name="color_background_selected_get_me">@android:color/white</item>
        <item name="color_background_ripple_get_me">@color/colorAccent</item>
        <item name="color_background_circle_get_me">@color/colorAccent</item>
        <item name="color_background_icon_get_me">@android:color/white</item>
        <item name="color_background_selection_check_icon_get_me">@color/colorAccent</item>
        <item name="color_background_selection_icon_get_me">@android:color/white</item>
        <item name="color_background_empty_directory_icon_get_me">@color/colorAccent</item>
        <item name="color_text_empty_directory_get_me">@color/colorAccent</item>
        <item name="color_text_get_me">@color/colorPrimaryDark</item>
        <item name="size_text_empty_directory">@dimen/size_text_empty_directory</item>
        <item name="string_empty_directory_get_me">@string/string_empty_directory_get_me</item>
    </style>

</resources>